{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": [
    "source.organizeImports",
    "source.addMissingImports",
    "source.fixAll.eslint"
  ],
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "files.encoding": "utf8",
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "go.lintTool": "golangci-lint",
  "go.lintFlags": ["--fast"],
  "go.formatTool": "goimports",
  "go.useLanguageServer": true,
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform"
  },
  "typescript.tsdk": "frontend/node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "cSpell.words": [
    "ABAC",
    "ADOT",
    "awscli",
    "awslocal",
    "Cheatsheets",
    "CICD",
    "CISSP",
    "codegen",
    "codgen",
    "Contentful",
    "coverprofile",
    "CUST",
    "dbconfig",
    "dbname",
    "direnv",
    "Distro",
    "distroless",
    "ecommerce",
    "envrc",
    "Errorf",
    "Fanout",
    "Fargate",
    "Fatalf",
    "Flexbox",
    "Geolocation",
    "getkin",
    "GOARCH",
    "golangci",
    "GOPATH",
    "goroutines",
    "healthcheck",
    "initdb",
    "Inno",
    "Knative",
    "labstack",
    "localstack",
    "MAINT",
    "MTTA",
    "MTTD",
    "MTTI",
    "MTTR",
    "mysqladmin",
    "NACL",
    "nosuke",
    "NOTIF",
    "oapi",
    "oapimiddleware",
    "ogen",
    "otelslog",
    "otlplog",
    "otlploghttp",
    "otlptrace",
    "otlptracehttp",
    "phpmyadmin",
    "Println",
    "Prio",
    "PRODPERF",
    "Referer",
    "rootpassword",
    "samber",
    "SCAL",
    "scrollbars",
    "semconv",
    "shopspring",
    "SIEM",
