{
  "id": null,
  "title": "Application Logs Overview",
  "description": "構造化ログの概要ダッシュボード",
  "tags": ["logs", "observability", "ecommerce"],
  "timezone": "Asia/Tokyo",
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "Log Volume by Level",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum by (level) (rate({service_name=\"aws-observability-ecommerce\"}[5m]))",
          "legendFormat": "{{level}}",
          "datasource": {
            "type": "loki",
            "uid": "loki"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Error Rate",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "rate({service_name=\"aws-observability-ecommerce\", level=\"error\"}[5m])",
          "datasource": {
            "type": "loki",
            "uid": "loki"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 0.1
              },
              {
                "color": "red",
                "value": 1
              }
            ]
          },
          "unit": "reqps"
        }
      }
    },
    {
      "id": 3,
      "title": "Request Status Code Distribution",
      "type": "piechart",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum by (status_code) (rate({service_name=\"aws-observability-ecommerce\", log_type=\"request\"} | json [5m]))",
          "legendFormat": "{{status_code}}",
          "datasource": {
            "type": "loki",
            "uid": "loki"
          }
        }
      ]
    },
    {
      "id": 4,
      "title": "Recent Error Messages",
      "type": "logs",
      "gridPos": {
        "h": 12,
        "w": 24,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "{service_name=\"aws-observability-ecommerce\", level=\"error\"} | json | line_format \"{{.timestamp}} [{{.level | upper}}] {{.message}} - {{.error.type}}\"",
          "datasource": {
            "type": "loki",
            "uid": "loki"
          }
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": false,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "prettifyLogMessage": false,
        "enableLogDetails": true,
        "dedupStrategy": "none",
        "sortOrder": "Descending"
      }
    },
    {
      "id": 5,
      "title": "Response Time Distribution",
      "type": "histogram",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 20
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate({service_name=\"aws-observability-ecommerce\", log_type=\"request\"} | json | unwrap duration_ms [5m]))",
          "legendFormat": "95th percentile",
          "datasource": {
            "type": "loki",
            "uid": "loki"
          }
        }
      ]
    },
    {
      "id": 6,
      "title": "Top Slow Endpoints",
      "type": "table",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 20
      },
      "targets": [
        {
          "expr": "topk(10, avg by (path) (rate({service_name=\"aws-observability-ecommerce\", log_type=\"request\"} | json | unwrap duration_ms [5m])))",
          "datasource": {
            "type": "loki",
            "uid": "loki"
          }
        }
      ]
    }
  ],
  "templating": {
    "list": [
      {
        "name": "service",
        "type": "constant",
        "current": {
          "value": "aws-observability-ecommerce",
          "text": "aws-observability-ecommerce"
        },
        "options": [
          {
            "value": "aws-observability-ecommerce",
            "text": "aws-observability-ecommerce"
          }
        ]
      }
    ]
  },
  "annotations": {
    "list": []
  },
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "links": [],
  "liveNow": false,
  "schemaVersion": 27,
  "style": "dark",
  "uid": "logs-overview",
  "version": 1
}
